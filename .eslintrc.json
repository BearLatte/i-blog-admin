{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential",
      "plugin:prettier/recommended"
    ],
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["vue"],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto",
          "semi": false,
          "singleQuote": true
        }
      ],
      "indent": ["error", 2],
      "camelcase": "off"
    },
    "ignorePatterns": ["node_modules", "build", "dist", "public"]
  }
  